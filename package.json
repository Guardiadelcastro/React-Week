{
  "name": "react-mini",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "webpack",
    "start": "node dist/bundle.js",
    "dev": "webpack-dev-server --mode development",
    "test": "jest"
  },
  "author": "Ale",
  "license": "ISC",
  "dependencies": {
    "react": "^16.9.0",
    "react-dom": "^16.9.0"
  },
  "devDependencies": {
    "@types/enzyme": "^3.10.3",
    "@types/jest": "^23.3.14",
    "@types/node": "^10.14.16",
    "@types/react": "^16.9.2",
    "@types/react-dom": "^16.9.0",
    "@types/webpack": "^4.39.1",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "jest": "^25.0.0",
    "jest-css-modules": "^2.1.0",
    "ts-jest": "^24.0.2",
    "ts-loader": "^4.3.0",
    "typescript": "^2.8.3",
    "webpack": "^4.39.3",
    "webpack-cli": "^3.3.7",
    "webpack-dev-server": "^3.8.0"
  },
  "jest": {
    "transform": {
      "^.+\\.(ts|tsx)$": "<rootDir>/config/jest/preprocessor.js"
    },
    "testPathIgnorePatterns": [
      "/node_modules/"
    ],
    "testRegex": "(/test/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "moduleNameMapper": {
      "\\.(css|jpg|png|svg)$": "<rootDir>/node_modules/jest-css-modules"
    },
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json"
    ],
    "setupFiles": [
      "<rootDir>/config/jest/setup.js"
    ]
  }
}
